{
  "name": "storefrontbackend",
  "version": "1.0.0",
  "main": "server.ts",
  "keywords": [],
  "author": "George Riad",
  "license": "ISC",
  "description": " online storefront backend",
  "scripts": {
    "test": "set NODE_ENV=test && db-migrate up --env=test && npx tsc && jasmine && db-migrate reset",
    "dev-up": "npx db-migrate up -m ./database/migrations   --config ./database/db.json",
    "dev-down": "npx db-migrate reset -m ./database/migrations  --config ./database/db.json",
    "dev": "nodemon ./src/server.ts",
    "clean": "rimraf build/",
    "build": "npm run  clean && npx tsc",
    "start": "npm run build && nodemon build/index.js",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^5.0.2",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.43",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "bcrypt": "^5.0.1",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-ts": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
